<p id="notice"><%= notice %></p>
<%= image_tag(@user.profile_picture.url(:medium)) %>
<p>
  <b>Email:</b>
  <%= @user.email %>
</p>

<p>
  <b>First name:</b>
  <%= @user.first_name %>
</p>

<p>
  <b>Last name:</b>
  <%= @user.last_name %>
</p>
<div class='text-center'>
<%= link_to 'Edit', edit_user_path(@user) %> |
<%= link_to 'Back', users_path %>
</div>
<%= display_tags(user) %>
