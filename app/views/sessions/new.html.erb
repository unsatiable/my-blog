<h1>Sign In</h1>

<%= form_tag create_session_path do %>
   <div class='feild'>
    <%= label_tag :email %>
    <%= text_field_tag :email, params[:email]%>
   </div>
   <div class='feild'>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
   </div>
   <%= submit_tag "Sign in" %>

  <% end %>